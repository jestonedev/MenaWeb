<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Мена жилья</title>
    <link rel="shortcut icon" href="~/exchange.png" type="image/x-icon">

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-datepicker/css/bootstrap-datepicker.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-datepicker/css/bootstrap-datepicker3.min.css" />
    <link rel="stylesheet" href="~/lib/jqueryui/jquery-ui.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-select/css/bootstrap-select.min.css" />
    <link rel="stylesheet" href="~/lib/open-iconic/font/css/open-iconic-bootstrap.min.css" />

    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

    @RenderSection("Styles", required: false)
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-toggleable-lg navbar-light bg-white border-bottom box-shadow mb-3">
        <div class="container">
            <a class="navbar-brand" asp-controller="Contract" asp-action="Index">
                <img src="~/exchange.png" width="30" height="30" alt="">
            </a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-lg-inline-flex flex-sm-row-reverse">
                <ul class="navbar-nav flex-grow-1">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownObjects" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Жилые объекты</a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownObjects">
                            <a class="dropdown-item" href="http://registry/Buildings/">Здания</a>
                            <a class="dropdown-item" href="http://registry/Premises/">Помещения</a>
                            <a class="dropdown-item" href="http://registry/RegistryObjectsReports/">Отчеты</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownOwners" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Собственность</a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownOwners">
                            <a class="dropdown-item" href="http://registry/OwnerProcesses/">Процессы собственности</a>
                            <a class="dropdown-item" href="http://registry/OwnerReasonTypes/">Типы оснований</a>
                            <a class="dropdown-item" href="http://registry/OwnerReports/">Отчеты</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownTenancies" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Найм жилья</a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownTenancies">
                            <a class="dropdown-item" href="http://registry/TenancyProcesses/">Процессы найма</a>
                            <a class="dropdown-item" href="http://registry/DocumentIssued/">Паспортные столы</a>
                            <a class="dropdown-item" href="http://registry/TenancyObjectsReports/">Отчеты</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownClaims" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Иск. работа (найм)</a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownClaims">
                            <a class="dropdown-item" href="http://registry/PaymentAccounts/">Лицевые счета БКС</a>
                            <a class="dropdown-item" href="http://registry/Claims/">Претензионно-исковые работы</a>
                            <a class="dropdown-item" href="http://registry/ClaimReports/">Отчеты</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownObjects" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Мена жилья</a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownObjects">
                            <a class="dropdown-item" asp-area="" asp-controller="Contract" asp-action="Index">Договоры мены</a>
                            <a class="dropdown-item" asp-area="" asp-controller="DocumentIssued" asp-action="Index">Паспортные столы</a>
                        </div>
                    </li>
                </ul>
            </div>
            <div class="ml-auto align-middle">
                @await Component.InvokeAsync("DatabaseNameComponent")
            </div>
            <div class="ml-auto align-middle pl-3">
                <u>@User.Identity.Name</u>
            </div>
        </div>
    </nav>
    <div class="container">
        @RenderBody()
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jqueryui/jquery-ui.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/bootstrap-datepicker/js/bootstrap-datepicker.min.js"></script>
    <script src="~/lib/bootstrap-datepicker/locales/bootstrap-datepicker.ru.min.js"></script>
    <script src="~/bootstrap-select/js/bootstrap-select.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/lib/jquery-inputmask/jquery.inputmask.js"></script>
    <script src="~/lib/jquery-inputmask/bindings/inputmask.binding.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
